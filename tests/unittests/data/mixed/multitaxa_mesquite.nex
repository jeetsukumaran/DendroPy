	FORMAT DATATYPE = DNA GAP = - MISSING = ?;
	MATRIX
	   x2.1 A
	   x2.2 A
	   x2.3 A
	   x2.4 A
	   ;
END;

BEGIN TREES;
	TITLE x2.trees1;
	LINK Taxa = x2;
	TRANSLATE
	    1 x2.1,
	    2 x2.2,
	    3 x2.3,
	    4 x2.4
	    ;
    TREE 1 = (1,(2,(3,4)));
    TREE 2 = (1,(2,(3,4)));
END;

